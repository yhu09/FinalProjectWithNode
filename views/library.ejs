/*NEW VERSION*/
div.gallery {
  width: 90%;
}

div.gallery img:hover {
  border: 2px solid #E8A87C;
}

div.gallery img {
  width: 100%;
  height: auto;
}

div.desc {
  padding: 15px;
  text-align: center;
  background-color: #ffe1c5;
  position: relative;
}

* {
  box-sizing: border-box;
}

.responsive {
  padding: 0 6px;
  float: left;
  width: 25%;
}

@media only screen and (max-width: 700px) {
  .responsive {
    width: 50%;
    margin: 6px 0;
  }
}

@media only screen and (max-width: 500px) {
  .responsive {
    width: 110%;
  }
}

.clearfix:after {
  content: "";
  display: table;
  clear: both;
}


</style>


